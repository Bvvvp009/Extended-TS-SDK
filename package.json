{
  "name": "extended-typescript-sdk",
  "version": "0.0.7",
  "description": "Community-maintained TypeScript SDK for Extended Exchange - A perpetual DEX built by ex-Revolut team",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "default": "./dist/esm/index.js"
    },
    "./package.json": "./package.json",
    "./wasm/*": "./wasm/*",
    "./utils/websocket": {
      "types": "./dist/esm/utils/websocket.d.ts",
      "import": "./dist/esm/utils/websocket.js",
      "require": "./dist/cjs/utils/websocket.js",
      "default": "./dist/esm/utils/websocket.js"
    },
    "./dist/esm/*": "./dist/esm/*",
    "./dist/cjs/*": "./dist/cjs/*"
  },
  "scripts": {
    "build": "npm run build:signer && npm run build:esm && npm run build:cjs && npm run build:post",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:post": "node scripts/post-build.js",
    "build:ts": "npm run build:esm && npm run build:cjs",
    "build:signer": "node scripts/build-signer.js",
    "build:signer:custom": "cd wasm-signer && wasm-pack build --target nodejs --out-dir ../wasm && wasm-pack build --target bundler --out-dir ../wasm-web-temp && node -e \"const fs=require('fs');['stark_crypto_wasm.js','stark_crypto_wasm_bg.wasm','stark_crypto_wasm.d.ts'].forEach(f=>{const src=`../wasm-web-temp/${f}`;const dest=`../wasm/${f.replace('.js','-web.js').replace('.wasm','-web.wasm').replace('.d.ts','-web.d.ts')}`;if(fs.existsSync(src))fs.copyFileSync(src,dest);});fs.rmSync('../wasm-web-temp',{recursive:true,force:true});\"",
    "clean": "rm -rf dist wasm wasm-signer/pkg wasm-signer/pkg-web wasm-signer/target",
    "prepublishOnly": "npm run build",
    "test": "npm run build && jest",
    "test:compat": "npm run build && jest tests/compat/",
    "test:cjs-esm": "npm run build && jest tests/compat/cjs-esm-wasm.test.js",
    "test:vite": "npm run build && jest tests/compat/vite-bundling.test.js",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "extended",
    "starknet",
    "trading",
    "perpetual",
    "dex",
    "sdk",
    "typescript",
    "web3"
  ],
  "author": "bvvvp009",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bvvvp009/Extended-TS-SDK.git"
  },
  "homepage": "https://github.com/Bvvvp009/Extended-TS-SDK#readme",
  "bugs": {
    "url": "https://github.com/Bvvvp009/Extended-TS-SDK/issues"
  },
  "dependencies": {
    "decimal.js": "^10.4.3",
    "dotenv": "^16.3.1",
    "ethers": "^6.9.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^20.11.0",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.4",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vite": "^7.3.0"
  },
  "files": [
    "dist",
    "wasm",
    "README.md",
    "vite.config.example.ts",
    "LICENSE"
  ]
}
